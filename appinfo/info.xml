<?xml version="1.0"?>
<info>
	<id>integration_pexip</id>
	<name>Pexip integration</name>
	<summary>Integration of Pexip video call service</summary>
	<description><![CDATA[The Pexip integration app implements a Smart Picker provider to create
Pexip meetings and immediately get a meeting link.

More information in the [project's README](https://github.com/julien-nc/integration_pexip/#readme).]]></description>
	<version>1.0.4</version>
	<licence>agpl</licence>
	<author>Julien Veyssier</author>
	<namespace>Pexip</namespace>
	<documentation>
		<developer>https://github.com/julien-nc/integration_pexip</developer>
	</documentation>
	<category>integration</category>
	<website>https://github.com/julien-nc/integration_pexip</website>
	<bugs>https://github.com/julien-nc/integration_pexip/issues</bugs>
	<screenshot>https://github.com/julien-nc/integration_pexip/raw/main/img/screenshots/screenshot1.jpg</screenshot>
	<screenshot>https://github.com/julien-nc/integration_pexip/raw/main/img/screenshots/screenshot2.jpg</screenshot>
	<screenshot>https://github.com/julien-nc/integration_pexip/raw/main/img/screenshots/screenshot3.jpg</screenshot>
	<screenshot>https://github.com/julien-nc/integration_pexip/raw/main/img/screenshots/screenshot4.jpg</screenshot>
	<dependencies>
		<nextcloud min-version="26" max-version="28"/>
	</dependencies>
	<background-jobs>
		<job>OCA\Pexip\Cron\CleanupCalls</job>
	</background-jobs>
	<commands>
		<command>OCA\Pexip\Command\CleanupCalls</command>
	</commands>
	<settings>
		<admin>OCA\Pexip\Settings\Admin</admin>
		<admin-section>OCA\Pexip\Settings\AdminSection</admin-section>
	</settings>
</info>
