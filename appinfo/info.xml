<?xml version="1.0"?>
<info>
    <id>integration_pexip</id>
    <name>Pexip integration</name>
    <summary>Integration of Pexip video call service</summary>
    <description><![CDATA[Pexip integration provides a smart picker provider to create
Pexip call and immediately get a link to this call.]]></description>
    <version>1.0.0</version>
    <licence>agpl</licence>
    <author>Julien Veyssier</author>
    <namespace>Pexip</namespace>
    <documentation>
        <developer>https://github.com/julien-nc/integration_pexip</developer>
    </documentation>
    <category>integration</category>
    <category>dashboard</category>
    <website>https://github.com/julien-nc/integration_pexip</website>
    <bugs>https://github.com/julien-nc/integration_pexip/issues</bugs>
    <screenshot>https://github.com/julien-nc/integration_pexip/raw/main/img/screenshot1.jpg</screenshot>
    <screenshot>https://github.com/julien-nc/integration_pexip/raw/main/img/screenshot2.jpg</screenshot>
    <screenshot>https://github.com/julien-nc/integration_pexip/raw/main/img/screenshot3.jpg</screenshot>
    <dependencies>
        <nextcloud min-version="26" max-version="27"/>
    </dependencies>
    <background-jobs>
		<job>OCA\Pexip\Cron\CleanupCalls</job>
	</background-jobs>
	<commands>
		<command>OCA\Pexip\Command\CleanupCalls</command>
	</commands>
    <settings>
        <admin>OCA\Pexip\Settings\Admin</admin>
        <admin-section>OCA\Pexip\Settings\AdminSection</admin-section>
    </settings>
</info>
